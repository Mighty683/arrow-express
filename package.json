    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "express": "4.21.2",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.7.3",
    "vitest": "^3.0.6",
    "vite": "^2.7.0"
